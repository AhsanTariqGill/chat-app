<h1>Log in</h1>

<div class="box">
  	<%= form_with(url: login_path, scope: :session, local: true) do |form| %>
  		<div>
    		<%= form.label :email %><br>
    		<%= form.email_field :email, class: 'form-control' %>
  		</div>
  		<div>
    		<%= form.label :password %><br>
    		<%= form.password_field :password, class: 'form-control' %>
  		</div>
  		<div>
    		<%= form.submit "Login"%>
  		</div>
	<% end %>
	
</div>
